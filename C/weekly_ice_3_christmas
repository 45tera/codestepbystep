// Online C compiler to run C program online
#include <stdio.h>

int main() {
        int n;
        printf("Enter n: ");
        scanf("%d",&n);
        
        int tr = n -5; //6
        
        for (int i =1; i <= tr; i++){ //rows
            int gap =tr-i;
            for (int p =0; p< gap;p++){ //cols
                printf(" ");
            }
            int limit = (2*i)-1;
            for (int q = 1; q <= limit ;q++){
                printf("*");
                
            }
            for (int p =0; p< gap;p++){ //cols
                printf(" ");
            }
            printf("\n");

        }
        
        int gap = tr -1;
        
        //branch
        for (int t =0; t <5; t++){
            for (int p =0; p< gap;p++){ //cols
                printf(" ");
            }
            printf("*");
            for (int p =0; p< gap;p++){ //cols
                printf(" ");
            }
            printf("\n");
        }
    return 0;
}
